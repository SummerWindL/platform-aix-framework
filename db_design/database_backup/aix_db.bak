CREATE FUNCTION aix.f_mb_user_auth_hosp_delete(in_userid character varying, in_hospcodelistjosn character varying, OUT retcode integer, OUT retvalue text) RETURNS record
    LANGUAGE plpgsql
    AS $$
/*
 * 函数说明：批量删除账号关联的医院表。
